{
    "manifest_version": 3,
    "name": "Open Phone MeetaOne",
    "version": "1.0.0",
    "description": "Abrir telefone de chamadas.",
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "background": {
      "service_worker": "src/background.js"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_icon": {
        "19": "icon19.png",
        "38": "icon38.png"
      },
      "default_title": "Open Phone MeetaOne"
    }
  }
  